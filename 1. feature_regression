     "output_type": "display_data"
    }
   ],
   "source": [
    "w0 = np.array([1.0,1.0,1.0]) #초기값\n",
    "_w = minimize(loss_mse,w0,args=(x,t))\n",
    "print(_w)\n",
    "\n",
    "ft = np.array([[1.,xval,xval**2] for xval in x])\n",
    "pred = np.dot(ft,_w.x) #행렬\n",
    "plt.plot(x,t,'o') \n",
    "plt.plot(x,pred,'--r') \n",
    "plt.grid()\n",
    "plt.xlabel('x')\n",
    "plt.ylabel('y')\n",
    "plt.show()"
