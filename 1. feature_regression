   "source": [
    "from scipy.optimize import minimize\n",
    "\n",
    "def loss_mse(w,x,t):\n",
    "    y = w[0] + w[1]*x + w[2]*(x**2)\n",
    "    val = 0.0\n",
    "    for n in range(len(x)):\n",
    "        val = val + np.square(t[n]-y[n])\n",
    "    val = val/x.shape[0]\n",
    "    return val"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
